@prefix agento: <http://example.org/agento#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix lg: <http://example.org/langgraph#> .

# Classes
agento:Workflow a rdfs:Class ;
    rdfs:label "Workflow" ;
    rdfs:comment "Represents a workflow or execution graph." .

agento:Node a rdfs:Class ;
    rdfs:label "Node" ;
    rdfs:comment "Represents a step/node in a workflow." .

# Object Properties
agento:callsNode a rdf:Property ;
    rdfs:domain agento:Workflow ;
    rdfs:range agento:Node ;
    rdfs:comment "Workflow has or calls a node." .

agento:hasStartNode a rdf:Property ;
    rdfs:domain agento:Workflow ;
    rdfs:range agento:Node ;
    rdfs:comment "Marks the start node of the workflow." .

agento:hasEndNode a rdf:Property ;
    rdfs:domain agento:Workflow ;
    rdfs:range agento:Node ;
    rdfs:comment "Marks the end node of the workflow." .

agento:follows a rdf:Property ;
    rdfs:domain agento:Workflow ;
    rdfs:range agento:Node ;
    rdfs:comment "Edge/sequence between nodes." .

# Datatype Properties
agento:nodeName a rdf:Property ;
    rdfs:domain agento:Node ;
    rdfs:range xsd:string ;
    rdfs:comment "Each node has a symbolic name." .

agento:callableLabel a rdf:Property ;
    rdfs:domain agento:Node ;
    rdfs:range xsd:string ;
    rdfs:comment "Label for the function or handler executed by the node." .

# Optional Properties
agento:vendorClass a rdf:Property ;
    rdfs:domain agento:Workflow ;
    rdfs:range xsd:string ;
    rdfs:comment "Allows cross-framework mapping of class names." .

# Instances
lg:aggregatorWorkflow a agento:Workflow ;
    agento:callsNode lg:aggregator_node ;
    agento:hasStartNode lg:aggregator_node ;
    agento:hasEndNode lg:aggregator_node .

lg:aggregator_node a agento:Node ;
    agento:nodeName "aggregator_node"^^xsd:string ;
    agento:callableLabel "lambda x: x"^^xsd:string .
